library(tidyverse)
library(knitr)
RawDataSet1 <- list.files(pattern = "xP26_") %>%
lapply(read_csv) %>%
bind_rows%>%
mutate(Subject= rep(list.files(pattern = "xP26_"), each=331))%>%
separate(`IT1 IT2 KEY RT`,c("IT1","a", "IT2","b","c","d","e","f","g","KEY", "RT"), sep = " ")%>%
select(Subject,IT1,IT2,KEY,RT)%>%
filter(IT1 != "S",
IT1 !="DESCANSO")%>%
mutate(Rubric = if_else(IT1 ==IT2, "m", "n"),
Accuracy = if_else(KEY == Rubric, 1, 0),
RT = as.numeric(RT))%>%
group_by(Subject)%>%
slice(4:329)%>%
ungroup()
RawDataSet2 <- list.files(pattern = "yP26_") %>%
lapply(read_csv) %>%
bind_rows%>%
mutate(Subject= rep(list.files(pattern = "yP26_"), each=329))%>%
separate(`IT1 IT2 KEY RT`,c("IT1","a", "IT2","b","c","d","e","f","g","KEY", "RT"), sep = " ")%>%
select(Subject,IT1,IT2,KEY,RT)%>%
filter(IT1 != "S",
IT1 !="DESCANSO")%>%
mutate(Rubric = if_else(IT1 ==IT2, "m", "n"),
Accuracy = if_else(KEY == Rubric, 1, 0),
RT = as.numeric(RT))%>%
group_by(Subject)%>%
slice(4:327)%>%
ungroup()
P26KaRaw<-RawDataSet1%>%
bind_rows(RawDataSet2)
save(P26KaRaw, file = "P28BeRaw.RData")
